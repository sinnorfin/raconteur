call pathogen#infect()
no <down> <Nop>
no <left> <Nop>
no <right> <Nop>
no <up> <Nop>

ino <left> <Nop>
ino <right> <Nop>
ino <up> <Nop>
ino <down> <Nop>

set number
set showcmd
syntax on
colorscheme wombat256
set hlsearch
filetype plugin indent on
set rtp+=~/.vim/bundle/powerline/bindings/vim
set laststatus=2
set modeline
set tabstop=4
set expandtab
set shiftwidth=4
set softtabstop=4
